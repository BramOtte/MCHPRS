{
  "name": "1t repeater",
  "description": "test the individual component behavior and the timing of a 1t repeater",
  "inputs": [
    "1,2,1"
  ],
  "outputs": [
    "3,2,1"
  ],
  "tests": [
    {
      "description": "test that the delay is 1t",
      "procedure": [
        "0: 0 0",
        "1: 1 0",
        "2: 1 1"
      ]
    },
    {
      "description": "test that the input pulse width is preserved",
      "procedure": [
        "0: 0 0",
        "1: 1 0",
        "2: 1 1",
        "3: 1 1",
        "4: 0 1",
        "5: 0 0"
      ]
    }
  ]
}